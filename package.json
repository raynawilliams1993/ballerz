    "bcrypt": "^4.0.1",
    "bcryptjs": "^2.4.3",
    "colors": "^1.4.0",
    "connect-flash": "^0.1.1",
    "express": "^4.17.1",
    "express-session": "^1.17.0",
    "if-env": "^1.0.4",
    "mongoose": "^5.9.3",
    "morgan": "^1.9.1",
    "node-sass": "^4.13.1",
    "passport": "^0.4.1",
    "passport-local": "^1.0.0",
    "path": "^0.12.7",
